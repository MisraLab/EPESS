function [ mu, Sigma, chol_Sigma, C, lB, uB ] = simulateTmg( dimension, axis_interval, distance_box_placement, inverse_wishart_df)


    % mean, covariances
    
       mu = zeros(dimension,1);
       lambda= rand;
       Sigma = (1-lambda) * eye(dimension) + lambda* iwishrnd(eye(dimension), inverse_wishart_df);
       
    % Specify Box constarints
        
       C = eye(n);         % For box constraints -- these will be identitiy
       lB = distance_box_placement + rand(n,1);    
       uB = rand(n,1) + axis_interval;
        
        
        
    

      chol_Sigma = chol(Sigma);
   





